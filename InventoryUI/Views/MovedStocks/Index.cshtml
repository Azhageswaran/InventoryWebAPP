<h1 class="mt-3">Moved Stocks History</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Moved Stock Id</th>
            <th>Raw Material Id</th>
            <th>Quantity</th>
            <th>Exit Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var movedStocksDtos in Model)
        {
            <tr>
                <td>@movedStocksDtos.MovedStockId</td>
                <td>@movedStocksDtos.RawMaterialId</td>
                <td>@movedStocksDtos.Quantity</td>
                <td>@movedStocksDtos.ExitDate</td>
                @*  <td>
            <a asp-controller="Regions" asp-action="Edit" asp-route-id="@region.Id" class="btn btn-light">Edit</a>
            </td>
            <td>
            <a asp-controller="Regions" asp-action="Del" asp-route-id="@region.Id" class="btn btn-danger">Del</a>
            </td> *@
            </tr>
        }
    </tbody>
</table>

<a asp-controller="RawMaterials" asp-action="Index" class="btn btn-light">Back</a>

